<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frontpage_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/lg" >

    <Button
        android:id="@+id/bsq"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="46dp"
        android:background="@drawable/button_custom"
        android:text="@string/sq"
        android:textColor="#ffffff"
        android:textSize="20sp" />

    <Button
        android:id="@+id/btq"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignBaseline="@+id/bsq"
        android:layout_alignBottom="@+id/bsq"
        android:layout_marginLeft="42dp"
        android:layout_toRightOf="@+id/bsq"
        android:background="@drawable/button_custom"
        android:text="@string/tq"
        android:textColor="#ffffff"
        android:textSize="20sp" />

    <Button
        android:id="@+id/brateapp"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/btq"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="51dp"
        android:background="@drawable/button_custom"
        android:text="@string/rateapp"
        android:textColor="#ffffff"
        android:textSize="20sp" />

    <Button
        android:id="@+id/bseeotherapps"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignBaseline="@+id/brateapp"
        android:layout_alignBottom="@+id/brateapp"
        android:layout_toLeftOf="@+id/btq"
        android:background="@drawable/button_custom"
        android:text="@string/seeother"
        android:textColor="#ffffff"
        android:textSize="20sp" />

</RelativeLayout>